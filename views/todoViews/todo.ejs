<!DOCTYPE html>
<html>
  <head>
    <title>Todo</title>
    <!-- Latest compiled and minified CSS -->
    <script src="https://code.jquery.com/jquery-1.12.1.min.js"></script>
    <script type="text/javascript" src="/assets/todo/js/todo.js" href=""></script>
    <link rel="stylesheet" href="/assets/todo/styles.css" type="text/css">
  </head>
  <body>
    <h1>Collin's Todo List</h1>

    <div id="todo-table">
      <form>
          <input type="text" name="item" placeholder="Add an Item" required/>
          <button type="submit">Add Item</button>
      </form>
      <ul>
        <% for(var i=0; i< todos.length; i++){ %>
            <li><%= todos[i].item %></li>
        <% } %>
      </ul>
    </div>

  </body>
</html>
